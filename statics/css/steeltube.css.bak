.zsjm{width:240px;margin-top:10px;}
.zsjm h1{width:210px;height:33px;line-height:34px;background:url(../images/title2.png);font-size:14px;color:#2f77c3;padding-left:30px;}
.zsjm .intro{width:238px;height:113px;padding-top:10px;border:1px solid #dfdfdf;border-top:none;border-bottom:none;}
.zsjm .intro img{display:block;float:left;margin-left:10px;}

.zsjm .intro p{width:100px;padding-right:8px;text-align:justify;text-justify:inter-ideograph;float:right;text-indent:2em;line-height:18px;color:#696969;}
.zsjm .intro p a{color:#2f77c3;}
.zsjm .intro p a:hover{color:#2f77c3;text-decoration:underline;}

.zsjm .list{border:1px solid #dfdfdf;border-top:none;width:238px;height:175px;}
.zsjm .list ul{}
.zsjm .list ul li{width:238px;float:left;margin-top:10px;}
.zsjm .list ul li img{display:block;float:left;margin-left:10px;margin-top:3px;_margin-left:5px;}
.zsjm .list ul li a{display:block;float:left;margin-left:5px;color:#696969;}
.zsjm .list ul li a:hover{color:#2f77c3;text-decoration:underline;}


/*友情链接*/
.web_nav{border:1px solid #dfdfdf;width:1004px;margin:0 auto}
.web_nav .web_nav_left{height:206px;width:160px;background:url("../images/tit-bg-bdr.png") repeat-y right 0;overflow:hidden; zoom:1;}
.web_nav .web_nav_right{height:206px;width:838px;overflow:hidden;zoom:1;position:relative;background:none;}

.web_nav_left .web_n_img{display:block; height:104px;background:url("../images/wzdh.png") no-repeat 0 0; width:25px; margin:1px 0 0 1px;}
.web_nav_left .web_n_tabs{width:133px;height:104px;margin:1px 0 ; overflow:hidden;zoom:1;}
.web_nav_left .web_n_tabs a{display:block;width:133px; *width:132px; text-align:center; height:26px;line-height:26px;background:url("../images/wzdh-bg.png") repeat-x 0 0;}
.web_nav_left .web_n_tabs a:hover,.web_nav_left .web_n_tabs a.current{background:url("../images/wzdh-current.png") repeat-x 0 0;color:#2f77c3;}


.web_nav_right .link_item{padding:5px;position:absolute;width:828px;height:95px;}

.web_nav_right .link_item .flink___{float: left; height: 95px; margin-right: 18px; overflow: hidden; width: 120px;}
.web_nav_right .link_item .flink___ a img{height:70px;width:120px;overflow:hidden;}


.list_con {overflow:hidden;zoom:1;}
.list_con .item_tit{width:100%;font-family:'微软雅黑';font-size:14px;background:url("../images/tit-bg-bdr.png") repeat-x 0 bottom;border-bottom:1px solid #2f77c3;}
.list_con .item{border:1px solid #2f77c3; border-width:0 1px 1px 1px;}
.list_con .item_tit,.list_con .item{overflow:hidden;font-size:12px;}
.list_con .item_tit ,.list_con .item_tit .tit_na{height:30px; line-height:30px;}
.list_con .item_tit .tit_na{display:block;float:left;text-align:left;font-size:14px; font-family:'微软雅黑';color:#fff;}
.list_con .item{}


#topic_foot_v2{background:#c8c8cc;margin-top:10px}
#topic_foot_v2 .link{width:1000px;margin:auto;padding:20px 0;font-size:12px}
#topic_foot_v2 .link li{width:100px;float:left;line-height:24px;text-align:center}
#topic_foot_v2 .link li a{display:block}
#common_bottom_v2{height:100px;background:#202020;overflow:hidden;}
#common_bottom_v2 .info{color:#ffffff;width:1000px;margin:auto;font-size:12px;line-height:22px}
#common_bottom_v2 .info a,#common_bottom_v2 .info span{color:#ffffff;padding:0 7px}
#common_bottom_v2 .info .link{float:left;margin:15px 0}
#common_bottom_v2 .info .link1{float:right;margin:15px 0}











